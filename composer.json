{
    "name": "vormkracht10/filament-translations",
    "description": "This is the official Vormkracht10 Filament Translations package!",
    "keywords": [
        "vormkracht10",
        "laravel",
        "filament-translations"
    ],
    "homepage": "https://github.com/vormkracht10/filament-translations",
    "support": {
        "issues": "https://github.com/vormkracht10/filament-translations/issues",
        "source": "https://github.com/vormkracht10/filament-translations"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Manoj Hortulanus",
            "email": "manoj@vormkracht10.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^3.0",
        "guava/filament-icon-select-column": "^1.1",
        "outhebox/blade-flags": "^1.5",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-translation-loader": "^2.8",
        "vormkracht10/laravel-translations": "dev-main"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Vormkracht10\\FilamentTranslations\\": "src/",
            "Vormkracht10\\FilamentTranslations\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vormkracht10\\FilamentTranslations\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vormkracht10\\FilamentTranslations\\FilamentTranslationsServiceProvider"
            ],
            "aliases": {
                "FilamentTranslations": "Vormkracht10\\FilamentTranslations\\Facades\\FilamentTranslations"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
