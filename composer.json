{
    "name": "backstage/translations",
    "description": "This is the official Backstagephp Translations package!",
    "keywords": [
        "backstagephp",
        "laravel",
        "filament",
        "translations"
    ],
    "homepage": "https://github.com/backstagephp/translations",
    "support": {
        "issues": "https://github.com/backstagephp/translations/issues",
        "source": "https://github.com/backstagephp/translations"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Manoj Hortulanus",
            "email": "manoj@vormkracht10.nl",
            "role": "Developer"
        },
        {
            "name": "Mark",
            "email": "mark@vormkracht10.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.6",
        "filament/filament": "^3.0",
        "ryangjchandler/filament-progress-column": "*",
        "spatie/laravel-package-tools": "*",
        "spatie/laravel-translation-loader": "^2.8"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Backstage\\Translations\\Filament\\": "src/",
            "Backstage\\Translations\\Filament\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backstage\\Translations\\Filament\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backstage\\Translations\\Filament\\TranslationsServiceProvider"
            ],
            "aliases": {
                "Translations": "Backstage\\Translations\\Filament\\Facades\\Translations"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
